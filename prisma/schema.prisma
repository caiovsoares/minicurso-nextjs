// npx prisma generate
// npx prisma db push

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model noticia {
  id        String @id @unique @default(uuid())
  titulo    String
  descricao String
  conteudo  String
  imagem    String
}
